{
  "name": "software-designs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "prestart": "npm run build",
    "start": "node",
    "pretest": "npm run build",
    "test": "node --test",
    "start:abstract-factory": "npm run start -- dist/OOP/patterns/creational/abstract-factory.js",
    "start:prototype": "npm run start -- dist/OOP/patterns/creational/prototype.js",
    "start:builder": "npm run start -- dist/OOP/patterns/creational/builder.js",
    "start:adapter": "npm run start -- dist/OOP/patterns/structural/adapter.js",
    "start:facade": "npm run start -- dist/OOP/patterns/structural/facade.js",
    "start:decorator": "npm run start -- dist/OOP/patterns/structural/decorator.js",
    "start:srp": "npm run start -- dist/OOP/SOLID/single-responsibility.js",
    "start:ocp": "npm run start -- dist/OOP/SOLID/open-closed.js",
    "start:lsp": "npm run start -- dist/OOP/SOLID/liskov-substitution.js",
    "start:isp": "npm run start -- dist/OOP/SOLID/interface-segregation.js",
    "start:dip": "npm run start -- dist/OOP/SOLID/dependency-inversion.js",
    "start:duck-typing": "npm run start -- dist/OOP/duck-typing.js",
    "start:lod": "npm run start -- dist/OOP/loose-coupling-and-high-cohesion.js",
    "start:binary-search-tree": "npm run start -- dist/data-structures/binary-tree.js",
    "start:prefix-tree": "npm run start -- dist/data-structures/prefix-tree.js",
    "test:prefix-tree": "npm run test -- dist/data-structures/prefix-tree.spec.js",
    "start:binary-search": "npm run start -- dist/algorithms/binary-search.js",
    "test:binary-search": "npm run test -- dist/algorithms/binary-search.spec.js"
